<!--pages/placeHolderHome/placeHolderHome.wxml-->

<view>
  <view wx:if="{{ active=='healers' }}">
    <init-place-holder wx:if="{{ originPlace.holderOpenId!=undefined }}"></init-place-holder>
    <text wx:else>已提交申请，请等待审核</text>
  </view>
  
  <!-- <healers wx:if="{{ active=='healers' }}"></healers> -->
  <ticket wx:elif="{{ active=='ticket' }}"></ticket>
  <to-develop wx:else="{{ active=='i' }}"></to-develop>
</view>


<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item name="healers" icon="friends-o">疗愈师们</van-tabbar-item>
  <van-tabbar-item name="ticket" icon="bill-o">票据</van-tabbar-item>
  <van-tabbar-item name="i" icon="smile-o">我</van-tabbar-item>
</van-tabbar>
